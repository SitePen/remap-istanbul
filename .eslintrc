{
  "extends": "airbnb",
  "env": {
    "es6": true,
    "browser": true,
    "jasmine": true,
    "node": true
  },
  "rules": {
    "jsx-no-bind": 0,
    "react/jsx-no-bind": 0,
    "prefer-template": 1,
    "max-len": [
      "error",
      120
    ],
    "no-restricted-syntax": 0,
    "no-param-reassign": 1,
    "global-require": 1,
    "no-underscore-dangle": 1,
    "no-shadow": 1,
    "consistent-return": 1
  }
}
